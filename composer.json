{
	"name" : "inneair/transaction-bundle",
	"type" : "symfony-bundle",
	"description" : "Transaction bundle for Symfony",
	"keywords" : [
		"transaction",
		"bundle",
		"symfony",
		"aop",
		"inneair",
		"tx"
	],
	"license" : "Apache-2.0",
	"authors" : [{
			"name" : "Inneair",
			"homepage" : "https://www.github.com/Inneair"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Inneair\\TransactionBundle\\" : "src/"
		}
	},
	"require" : {
		"php" : ">=5.6",
		"jms/aop-bundle" : "~1.0",
		"symfony/config" : "~2.3",
		"symfony/dependency-injection" : "~2.3",
		"symfony/doctrine-bridge" : "~2.3",
		"symfony/http-kernel" : "~2.3",
		"doctrine/orm" : "~2.4",
		"psr/log" : "~1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4.0",
		"phpdocumentor/phpdocumentor" : "~2.0",
		"satooshi/php-coveralls" : "~0.6"
	},
	"config" : {
		"bin-dir" : "bin"
	},
	"minimum-stability" : "stable",
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.0.x-dev"
		}
	}
}